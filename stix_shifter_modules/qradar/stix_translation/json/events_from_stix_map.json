{
  "ipv4-addr": {
    "fields": {
      "value": ["sourceip", "destinationip", "identityip"]
    }
  },
  "ipv6-addr": {
    "fields": {
      "value": ["sourceip", "destinationip"]
    }
  },
  "url": {
    "fields": {
      "value": ["url"]
    }
  },
  "mac-addr": {
    "fields": {
      "value": ["sourcemac", "destinationmac"]
    }
  },
  "file": {
    "fields": {
      "name": ["filename"],
      "hashes.SHA-256": ["filehash"],
      "hashes.MD5": ["filehash"],
      "hashes.SHA-1": ["filehash"],
      "parent_directory_ref": ["filepath"],
      "parent_directory_ref.path": ["filepath"]
    }
  },
  "directory": {
    "fields": {
      "path": ["filepath"]
    }
  },
  "network-traffic": {
    "fields": {
      "src_port": ["sourceport"],
      "dst_port": ["destinationport"],
      "protocols[*]": ["protocolid"],
      "start": ["starttime"],
      "end": ["endtime"],
      "src_ref.value": ["sourceip", "sourcemac"],
      "dst_ref.value": ["destinationip", "destinationmac"]
    }
  },
  "user-account": {
    "fields": {
      "user_id": ["username"]
    }
  },
  "artifact": {
    "fields": {
      "payload_bin": ["eventpayload"]
    }
  },
  "domain-name": {
    "fields": {
      "value": ["domainname"]
    }
  },
  "x-ibm-ariel": {
    "fields": {
      "qid": ["qid"],
      "event_name": ["qidname"],
      "magnitude": ["magnitude"],
      "log_source_id": ["logsourceid"],
      "device_type": ["devicetype"],
      "log_source_name": ["logsourcename"],
      "log_source_type_name": ["logsourcetypename"],
      "category_id": ["categoryid"],
      "category_name": ["categoryname"],
      "high_level_category_name": ["high_level_category_name"],
      "high_level_category_id": ["high_level_category_id"],
      "direction": ["direction"],
      "identity_host_name": ["identityhostname"],
      "severity": ["severity"],
      "credibility": ["credibility"],
      "relevance": ["relevance"],
      "geographic": ["geographic"],
      "domain_id": ["domainid"],
      "rule_names[*]": ["rulenames"]
    }
  },
  "x-ibm-finding": {
    "fields": {
      "name": ["crename"],
      "description": ["credescription"]
    }
  },
  "process": {
    "fields": {
      "image_ref.name": ["Image"],
      "image_ref.parent_directory_ref.path": ["Image"],
      "parent_ref.image_ref.name": ["ParentImage"],
      "command_line": ["ProcessCommandLine"],
      "parent_ref.command_line": ["ParentCommandLine"],
      "extensions.windows-service-ext.service_dll_refs[*].name": ["ServiceFileName", "LoadedImage"]
    }
  },
  "x-event": {
    "fields": {
      "id": ["EventID"],
      "code": ["EventID"],
      "action": ["qidname"],
      "original": ["Message"]
    }
  },
  "windows-registry-key": {
    "fields": {
      "key": ["ObjectName"],
      "values[*].data": ["RegistryValueData"],
      "values[*].name": ["RegistryValueName"]

    }
  },
  "x-windows": {
    "fields": {
      "initiatedconnection": [
        "InitiatedConnection"
      ],
      "targetimage": [
        "TargetImage"
      ],
      "grantedaccess": [
        "GrantedAccess"
      ],
      "calltrace": [
        "CallTrace"
      ],
      "sourceimage": [
        "Image"
      ],
      "pipename": [
        "PipeName"
      ],
      "startmodule": [
        "StartModule"
      ],
      "startfunction": [
        "StartFunction"
      ],
      "signed": [
        "Signed"
      ],
      "imphash": [
        "IMPHash"
      ],
      "startaddress": [
        "StartAddress"
      ],
      "integritylevel": [
        "IntegrityLevel"
      ]
    }
  }
}
